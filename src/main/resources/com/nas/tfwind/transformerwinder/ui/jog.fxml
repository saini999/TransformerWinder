<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="240.0" prefWidth="240.0" stylesheets="@../css/bootstrapfx.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nas.tfwind.transformerwinder.ui.jogController">
   <children>
      <Button fx:id="yNeg" layoutX="10.0" layoutY="53.0" minWidth="62.0" mnemonicParsing="false" onAction="#yNeg" prefHeight="16.0" prefWidth="51.0" text="&lt; Y-" wrapText="true">
         <styleClass>
            <String fx:value="btn-primary" />
            <String fx:value="btn" />
         </styleClass></Button>
      <Button fx:id="yPos" layoutX="150.0" layoutY="53.0" minWidth="51.0" mnemonicParsing="false" onAction="#yPos" prefHeight="16.0" prefWidth="51.0" text="Y+ &gt;">
         <styleClass>
            <String fx:value="btn-primary" />
            <String fx:value="btn" />
         </styleClass></Button>
      <Label fx:id="jg" alignment="CENTER" layoutX="99.0" layoutY="57.0" prefHeight="23.0" prefWidth="44.0" styleClass="lbl-default" text="JOG" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="ss" alignment="CENTER" layoutX="11.0" layoutY="94.0" prefHeight="23.0" prefWidth="70.0" styleClass="lbl-default" text="Step SIze" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="stepSize" layoutX="78.0" layoutY="94.0" onAction="#stepSize" prefHeight="23.0" prefWidth="70.0" promptText="mm" />
      <Button fx:id="setZero" layoutX="151.0" layoutY="90.0" minWidth="70.0" mnemonicParsing="false" onAction="#setZero" prefHeight="30.0" prefWidth="70.0" text="Set Zero">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-danger" />
         </styleClass>
      </Button>
      <Button fx:id="gotoZero" layoutX="112.0" layoutY="133.0" mnemonicParsing="false" onAction="#gotoZero" text="Return to Zero">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-primary" />
         </styleClass>
      </Button>
      <Button fx:id="gotoEnd" layoutX="124.0" layoutY="174.0" mnemonicParsing="false" onAction="#gotoEnd" prefHeight="30.0" prefWidth="93.0" text="Goto End">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-success" />
         </styleClass>
      </Button>
      <CheckBox fx:id="reverseDir" layoutX="62.0" layoutY="210.0" mnemonicParsing="false" onAction="#reverseDir" text="Reverse Direction" />
      <Button fx:id="gotoWork" layoutX="17.0" layoutY="174.0" mnemonicParsing="false" onAction="#gotoWork" text="Goto Work">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-success" />
         </styleClass>
      </Button>
      <Button fx:id="setWork" layoutX="22.0" layoutY="133.0" mnemonicParsing="false" onAction="#setWork" text="Set Work">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-danger" />
         </styleClass>
      </Button>
      <Button fx:id="enableJog" layoutX="76.0" layoutY="14.0" mnemonicParsing="false" onAction="#enableJog" text="Enable Jog">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-danger" />
         </styleClass>
      </Button>
   </children>
</AnchorPane>
